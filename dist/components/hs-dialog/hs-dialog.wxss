.sg-mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.5); }

.sg-mask_transparent {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1000; }

.sg-hs-dialog,
.sg-mask {
  -webkit-transition: all 0.3s;
  transition: all 0.3s; }

.sg-hidden .sg-mask {
  visibility: hidden;
  opacity: 0; }

.sg-hidden .sg-hs-dialog {
  -webkit-transform: translateY(100%);
          transform: translateY(100%); }

.sg-show .sg-mask {
  visibility: visible;
  opacity: 1; }

.sg-show .sg-hs-dialog {
  -webkit-transform: translateY(0);
          transform: translateY(0); }

.sg-hs-dialog {
  position: fixed;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 5000;
  max-height: 75%;
  padding: 0 48rpx;
  padding: 0 calc(48rpx + constant(safe-area-inset-right)) constant(safe-area-inset-bottom) calc(48rpx + constant(safe-area-inset-left));
  padding: 0 calc(48rpx + env(safe-area-inset-right)) env(safe-area-inset-bottom) calc(48rpx + env(safe-area-inset-left));
  overflow: hidden;
  line-height: 1.4;
  background-color: #fff;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  -webkit-transition: all 0.3s;
  transition: all 0.3s; }

.sg-hs-dialog__hd {
  display: -webkit-box;
  display: flex;
  -webkit-box-align: center;
          align-items: center;
  height: 140rpx;
  font-size: 16rpx; }
  .sg-hs-dialog__hd .sg-icon-btn {
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%); }

.sg-hs-dialog__hd__side {
  position: relative;
  left: -16rpx; }

.sg-hs-dialog__hd__main {
  -webkit-box-flex: 1;
          flex: 1; }
  .sg-hs-dialog__hd__side + .sg-hs-dialog__hd__main {
    padding: 0 80rpx;
    text-align: center; }
  .sg-hs-dialog__hd__main + .sg-hs-dialog__hd__side {
    right: -16rpx;
    left: auto; }
    .sg-hs-dialog__hd__main + .sg-hs-dialog__hd__side .sg-icon-btn {
      right: 0; }

.sg-hs-dialog__title {
  display: block;
  color: #303133;
  font-weight: 600;
  font-size: 30rpx;
  text-align: center; }

.sg-hs-dialog__bd {
  word-wrap: break-word;
  -webkit-hyphens: auto;
  -ms-hyphens: auto;
      hyphens: auto;
  overflow-y: auto;
  color: #303133; }

.sg-hs-dialog__ft {
  padding: 72rpx 0;
  text-align: center; }
  .sg-hs-dialog__ft .sg-btn:nth-last-child(n+2),
  .sg-hs-dialog__ft .sg-btn:nth-last-child(n+2) + .sg-btn {
    display: inline-block;
    width: 240rpx;
    margin: 0 16rpx;
    vertical-align: top; }

.sg-icon-btn {
  font-size: 0;
  background-color: transparent;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  background-size: 100%;
  border: 0;
  outline: 0; }

